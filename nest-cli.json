{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/rockets/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/rockets/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/rockets",
  "projects": {
    "messages-service": {
      "type": "application",
      "root": "apps/messages-service",
      "entryFile": "main",
      "sourceRoot": "apps/messages-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/messages-service/tsconfig.app.json"
      }
    },
    "rockets": {
      "type": "application",
      "root": "apps/rockets",
      "entryFile": "main",
      "sourceRoot": "apps/rockets/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rockets/tsconfig.app.json"
      }
    },
    "rockets-message-consumer": {
      "type": "application",
      "root": "apps/rockets-message-consumer",
      "entryFile": "main",
      "sourceRoot": "apps/rockets-message-consumer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rockets-message-consumer/tsconfig.app.json",
        "webpack": false,
        "assets": []
      }
    }
  }
}